<?xml version="1.0" encoding="utf-8"?>
<includes>
	<!-- rework start -->
	<default type="label">
		<label />
		<align>left</align>
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>white2</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<selectedcolor>$VAR[HighlightColorVar]</selectedcolor>
		<scroll>false</scroll>
		<scrollspeed>40</scrollspeed>
	</default>
	<default type="fadelabel">
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>white2</textcolor>
		<selectedcolor>$VAR[HighlightColorVar]</selectedcolor>
		<scrollspeed>40</scrollspeed>
		<pauseatend>7000</pauseatend>
		<scrollout>false</scrollout>
	</default>
	<default type="grouplist">
		<itemgap>0</itemgap>
	</default>
	<default type="image">
		<aspectratio>stretch</aspectratio>
		<texture />
	</default>
	<default type="multiimage">
		<fadetime>500</fadetime>
		<timeperimage>7000</timeperimage>
	</default>
	<default type="button">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">button/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">button/list-select-nofocus.png</texturenofocus>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>white2</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="radiobutton">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">button/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">button/list-select-nofocus.png</texturenofocus>
		<textureradioon>button/radiobutton-on.png</textureradioon>
		<textureradiooff>button/radiobutton-off.png</textureradiooff>
		<radiowidth>62</radiowidth>
		<radioheight>35</radioheight>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>white2</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="togglebutton">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">button/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">button/list-select-nofocus.png</texturenofocus>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>white2</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="spincontrol">
		<textureup>button/spin-up-nofocus.png</textureup>
		<texturedown>button/spin-down-nofocus.png</texturedown>
		<textureupfocus>button/spin-up-focus.png</textureupfocus>
		<texturedownfocus>button/spin-down-focus.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">button/spin-up-nofocus.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">button/spin-down-nofocus.png</texturedowndisabled>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>white2</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="spincontrolex">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">button/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">button/list-select-nofocus.png</texturenofocus>
		<textureup>button/spin-up-nofocus.png</textureup>
		<texturedown>button/spin-down-nofocus.png</texturedown>
		<textureupfocus>button/spin-up-focus.png</textureupfocus>
		<texturedownfocus>button/spin-down-focus.png</texturedownfocus>
		<spinwidth>32</spinwidth>
		<spinheight>35</spinheight>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>white2</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="scrollbar">
		<width>38</width>
		<texturesliderbackground />
		<texturesliderbar />
		<texturesliderbarfocus />
		<textureslidernib>button/scrollbar-nib-nofocus.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[HighlightColorVar]">button/scrollbar-nib-focus.png</textureslidernibfocus>
		<showonepage>false</showonepage>
		<orientation>vertical</orientation>
	</default>
	<default type="progress">
		<height>20</height>
		<reveal>true</reveal>
		<texturebg colordiffuse="$VAR[PanelColorVar]" border="8">dialog/progress/progress-back.png</texturebg>
		<lefttexture />
		<midtexture colordiffuse="$VAR[HighlightColorVar]" border="8">dialog/progress/progress-mid.png</midtexture>
		<righttexture />
		<overlaytexture />
	</default>
	<default type="slider">
		<texturesliderbar border="15">button/slider-back.png</texturesliderbar>
		<textureslidernib>button/slider-nofocus.png</textureslidernib>
		<textureslidernibfocus>button/slider-focus.png</textureslidernibfocus>
		<orientation>horizontal</orientation>
		<width>200</width>
		<height>34</height>
	</default>
	<default type="sliderex">
		<texturesliderbar border="15">button/slider-back.png</texturesliderbar>
		<textureslidernib>button/slider-nofocus.png</textureslidernib>
		<textureslidernibfocus>button/slider-focus.png</textureslidernibfocus>
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">button/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">button/list-select-nofocus.png</texturenofocus>
		<sliderwidth>200</sliderwidth>
		<sliderheight>34</sliderheight>
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>white2</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<!-- rework end -->


	
	<default type="textbox">
		<align>justify</align>
		<font>Font_Reg29</font>
		<textcolor>white2</textcolor>
	</default>

	
	<default type="mover">
		<left>0</left>
		<top>0</top>
		<texturefocus>-</texturefocus>
		<texturenofocus>-</texturenofocus>
	</default>
	<default type="resize">
		<left>0</left>
		<top>348</top>
		<width>384</width>
		<height>384</height>
		<texturefocus>calibration/calibrate-pixelratio-focus.png</texturefocus>
		<texturenofocus>calibration/calibrate-pixelratio.png</texturenofocus>
	</default>
	<default type="edit">
		<left>0</left>
		<top>0</top>
		<width>450</width>
		<height>63</height>
		<textoffsetx>10</textoffsetx>
		<aligny>center</aligny>
		<texturefocus>-</texturefocus>
		<texturenofocus>button-nofocus.png</texturenofocus>
		<font>Font_Reg29</font>
		<textcolor>white2</textcolor>
		<disabledcolor>grey3</disabledcolor>
	</default>
	
</includes>