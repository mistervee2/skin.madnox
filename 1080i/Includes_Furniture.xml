	<include name="Dialog_DefaultList">
		<param name="width">974</param>
		<param name="height">420</param>
		<definition>
			<left>20</left>
			<top>88</top>
			<width>$PARAM[width]</width>
			<height>$PARAM[height]</height>
			<onup>$PARAM[id]</onup>
			<ondown>$PARAM[id]</ondown>
			<onleft>60</onleft>
