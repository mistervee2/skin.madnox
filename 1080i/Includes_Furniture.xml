	<!--  Floor -->
	<include name="Floor_Background_Default">
		<control type="image">
			<width>464</width>
			<height>882</height>
			<texture colordiffuse="$VAR[BackgroundColorVar]">panel/floor-shade.png</texture>
		</control>
		<control type="image">
